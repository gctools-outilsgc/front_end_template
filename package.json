{
  "name": "front_end_template",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://username.github.io/repo",
  "dependencies": {
    "@babel/core": "7.3.3",
    "@gctools-components/aurora-css": "^0.3.3",
    "@gctools-components/eslint-config": "1.1.3",
    "@gctools-components/gc-login": "1.1.3",
    "@gctools-components/i18n-translation-webpack-plugin": "2.0.1",
    "@gctools-components/react-i18n-translation-webpack": "1.1.4",
    "@svgr/webpack": "4.1.0",
    "apollo-boost": "^0.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "classnames": "2.2.6",
    "css-loader": "2.1.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.14.1",
    "eslint-config-react-app": "3.0.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "gh-pages": "2.0.1",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "jest-pnp-resolver": "1.1.0",
    "jest-resolve": "24.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parse-svg-path": "^0.1.2",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-apollo": "2.4.1",
    "react-app-polyfill": "0.2.1",
    "react-dev-utils": "7.0.3",
    "react-dom": "16.8.3",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "react-router-prop-types": "1.0.4",
    "react-testing-library": "5.9.0",
    "reactstrap": "7.1.0",
    "redux": "4.0.1",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.29.5",
    "webpack-dev-server": "3.2.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test2": "jest --rootDir . --watchPathIgnorePatterns jest.json --testURL http://localhost --config ./node_modules/@gctools-components/jest-config/index.js",
    "lint": "eslint .",
    "predeploy": "npm run build",
    "deploy-gh-pages": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "@gctools-components",
    "root": true,
    "rules": {
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "never",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "controlComponents": [
            "Input"
          ]
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "json",
      "json-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "babel-polyfill",
      "<rootDir>/config/jest/i18n.js",
      "<rootDir>/config/jest/register-context.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/.storybook/Storyshots.test.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "env": {
      "test": {
        "plugins": [
          "require-context-hook"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-plugin-require-context-hook": "1.0.0",
    "react-test-renderer": "16.8.3"
  }
}
